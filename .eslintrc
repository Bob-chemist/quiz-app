{
  "extends": [
    "react-app",
    "prettier"
  ],
  "plugins": [
    "react-hooks",
    "prettier"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "prettier/prettier": ["error", {
      "singleQuote": true
    }],
    "jsx-quotes": ["error", "prefer-double"],
    "quotes": ["error", "single"]
  }
}